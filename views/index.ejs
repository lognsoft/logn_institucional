<body id="card-page-body">

<!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-52H4QX2B"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
    


    <!-- main-area -->
    <main id="card-main" class="main--area">

        

        <!-- services-details-area -->
        <!-- <section id="card-area" class="services__details-area section-pt-120 section-pb-120" data-background="/images/bg/team_details_bg.jpg"> -->
        <section id="card-area" class="services__details-area section-pt-120 section-pb-120" data-background="/images/bg/team_details_bg.jpg">
            <div class="container">
                <div class="row">
                    <div class="col-12">
                        <div class="team__details-content">
                            <span class="sub-title">Fabricamos softwares e treinamos pessoas</span>
                            <h2 class="title" style="font-size: 35px">O que você procura?</h2>
                        </div>
                    </div>
                </div>
                <div class="row justify-content-center" style="margin-top: 100px">
                    <div class="col-xl-3 col-lg-4 col-sm-6 wow fadeInUp" data-wow-delay=".2s">
                        <div class="team__item">
                            <div class="team__thumb">
                                <a href="/servicos"><img class="my-card-img" src="/images/service.svg" alt="img"></a>
                            </div>
                            <div class="team__content">
                                <h4 class="name"><a href="/servicos">Desenvolvimento</a></h4>
                                <span class="designation">Desktop / Mobile / Web</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-lg-4 col-sm-6 wow fadeInUp" data-wow-delay=".2s">
                        <div class="team__item">
                            <div class="team__thumb">
                                <a href="/educa"><img class="my-card-img" src="/images/curso.svg" alt="img"></a>
                            </div>
                            <div class="team__content">
                                <h4 class="name"><a href="/educa">Treinamento</a></h4>
                                <span class="designation">Conheça nossos cursos</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- services-details-area-end -->

    </main>
    <!-- main-area-end -->


    <!-- footer-start -->
    <footer id="card-area-footer" class="footer-style-one">
        <div class="copyright__wrap">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col text-center">
                        <div class="copyright__text">
                            <p><span>LOGN</span> © 2023</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </footer>
    <!-- footer-start-end -->
    <!-- JS here -->
    <script src="/javascripts/vendor/jquery-3.6.0.min.js"></script>
    <script src="/javascripts/bootstrap.min.js"></script>
    <script src="/javascripts/jquery.magnific-popup.min.js"></script>
    <script src="/javascripts/jquery.countdown.min.js"></script>
    <script src="/javascripts/jquery.waypoints.min.js"></script>
    <script src="/javascripts/jquery.odometer.min.js"></script>
    <script src="/javascripts/simpleParallax.min.js"></script>
    <script src="/javascripts/jquery-ui.min.js"></script>
    <script src="/javascripts/gsap.min.js"></script>
    <script src="/javascripts/scrollTrigger.min.js"></script>
    <script src="/javascripts/splitText.min.js"></script>
    <script src="/javascripts/jquery.appear.js"></script>
    <script src="/javascripts/tg-cursor.min.js"></script>
    <script src="/javascripts/swiper-bundle.js"></script>
    <script src="/javascripts/jarallax.min.js"></script>
    <script src="/javascripts/parallax.min.js"></script>
    <script src="/javascripts/slick.min.js"></script>
    <script src="/javascripts/ajax-form.js"></script>
    <script src="/javascripts/vivus.min.js"></script>
    <script src="/javascripts/wow.min.js"></script>
    <script src="/javascripts/main.js"></script>
</body>

</html>